{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "triple-equals": false,
      "one-variable-per-declaration": false,
      "no-bitwise": false,
      "no-trailing-whitespace": false,
      "prefer-const": false,
      "quotemark": false,
      "whitespace": false,
      "ordered-imports": false,
      "interface-name": false,
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "ignore",
            "typeLiterals": "ignore"
          },
          "esSpecCompliant": true
        }
      ]
    },
    "rulesDirectory": []
}